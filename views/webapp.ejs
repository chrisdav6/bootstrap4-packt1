  <!-- Profile Section -->
  <section class="py-5">
    <div class="container">
      
      <div class="row">
        <div id="profile" class="col-lg-3 col-md-4">
          
          <div class="card">
            <img src="img/landscape.jpg" class="img-fluid card-img-top">
            <div class="card-body">
              <img src="img/jon.png" class="card-img">
              <div class="card-title">
                <h5 class="mb-0">Johnny Doo</h5>
                <p><small>@johnnydoo</small></p>
              </div>
              <p class="card-text">Dog goes woofy! Did you say squirrel</p>
              <ul class="list-inline stats">
                <li class="list-inline-item card-tweets">
                  <a href="#">
                    <span class="profile-stats">Tweets</span>
                    <span class="profile-value">99k</span>
                  </a>
                </li>
                <li class="list-inline-item card-following">
                  <a href="#">
                    <span class="profile-stats">Following</span>
                    <span class="profile-value">7</span>
                  </a>
                </li>
                <li class="list-inline-item card-followers">
                  <a href="#">
                    <span class="profile-stats">Followers</span>
                    <span class="profile-value">100</span>
                  </a>
                </li>
              </ul>
            </div>
          </div><!-- End of Card -->
          
          <div id="profilePhoto" class="card mt-4">
            <div class="card-header">Photos</div>
            <div class="card-body">

              <a href="img/landscape-02.jpg">
                <img class="img-thumbnail" src="img/landscape-02.jpg">
              </a>

              <a href="img/landscape-03.jpg">
                <img class="img-thumbnail" src="img/landscape-03.jpg">
              </a>

              <a href="img/landscape-04.jpg">
                <img class="img-thumbnail" src="img/landscape-04.jpg">
              </a>

              <a href="img/landscape-05.jpg">
                <img class="img-thumbnail" src="img/landscape-05.jpg">
              </a>

            </div>
          </div><!-- End of Card -->
      
        </div><!-- End of col-lg-3 col-md-4 -->
        
        <div class="col-lg-9 col-md-8">
            <!-- Nested row -->
            <div class="row">
              
              <div id="main" class="col-xl-8 mt-4 mt-md-0">
                <div class="alert alert-primary mb-3" role="alert">
                  <form id="new-message">
                    <div class="input-group">
                      <input type="text" class="form-control" placeholder="What is happening?">
                      <div class="input-group-append">
                        <span class="input-group-text" id="basic-addon1"><i class="fas fa-camera"></i></span>
                      </div>
                    </div>
                  </form>
                </div>
                
                <!-- Posts -->
                <div class="card">
                  <div class="card-body">
                    <ul id="feed" class="list-unstyled">
                      <li>
                        <img src="img/doge.jpg" class="feed-avatar rounded-circle">
                        <div class="feed-post">
                          <h5 class="mb-1">Doge <small>@dogeofficial - 3h</small></h5>
                          <p>You can't hold a dog down without staying down with him!</p>
                        </div>
                        <div class="action-list">
                          <a href="#">
                            <i class="fas fa-share"></i>
                          </a>
                          <a href="#">
                            <i class="fas fa-retweet"></i>
                            <span class="retweet-count">6</span>
                          </a>
                          <a href="#">
                            <i class="fas fa-star"></i>
                          </a>
                        </div>
                      </li>
                    </ul>
                  </div>
                </div><!-- End of Card -->
                
                <div class="card">
                  <div class="card-body">
                    <ul id="feed" class="list-unstyled">
                      <li>
                        <img src="img/laika.jpg" class="feed-avatar rounded-circle">
                        <div class="feed-post">
                          <h5 class="mb-1">Laika <small>@spaceegg - 4h</small></h5>
                          <p>You can't hold a dog down without staying down with him!</p>
                        </div>
                        <div class="action-list">
                          <a href="#">
                            <i class="fas fa-share"></i>
                          </a>
                          <a href="#">
                            <i class="fas fa-retweet"></i>
                            <span class="retweet-count">6</span>
                          </a>
                          <a href="#">
                            <i class="fas fa-star"></i>
                          </a>
                        </div>
                      </li>
                    </ul>
                  </div>
                </div><!-- End of Card -->
                
                <nav class="pt-4">
                  <ul class="pagination justify-content-center">
                    <li class="page-item disabled">
                      <a class="page-link" href="#" tabindex="-1">&laquo;</a>
                    </li>
                    <li class="page-item"><a class="page-link" href="#">1</a></li>
                    <li class="page-item"><a class="page-link" href="#">2</a></li>
                    <li class="page-item"><a class="page-link" href="#">3</a></li>
                    <li class="page-item">
                      <a class="page-link" href="#">&raquo;</a>
                    </li>
                  </ul>
                </nav>
                
      
                
              </div><!-- End of col-xl-8 in nested row -->
              
              <div id="main" class="col-xl-4 mt-3 mt-xl-0">
                <div class="alert alert-success" role="alert">
                  <p>Who to follow</p>
                </div>
              </div><!-- End of col-xl-4 in nested row -->
              
            </div><!-- End of nested row -->
        </div><!-- End of col-lg-9 col-md-8 -->

      </div><!-- End of Row -->
    
      
      
    </div>
  </section>
  <!-- End of Profile Section -->

