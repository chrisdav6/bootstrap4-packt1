  <!-- Profile Section -->
  <section class="py-5">
    <div class="container">
      
      <div class="row">
        <div id="profile" class="col-lg-3 col-md-4">
          
          <div class="card">
            <img src="img/landscape.jpg" class="img-fluid card-img-top">
            <div class="card-body">
              <img src="img/jon.png" class="card-img">
              <div class="card-title">
                <h5 class="mb-0">Johnny Doo</h5>
                <p><small>@johnnydoo</small></p>
              </div>
              <p class="card-text">Dog goes woofy! Did you say squirrel</p>
              <ul class="list-inline stats">
                <li class="list-inline-item card-tweets">
                  <a href="#">
                    <span class="profile-stats">Tweets</span>
                    <span class="profile-value">99k</span>
                  </a>
                </li>
                <li class="list-inline-item card-following">
                  <a href="#">
                    <span class="profile-stats">Following</span>
                    <span class="profile-value">7</span>
                  </a>
                </li>
                <li class="list-inline-item card-followers">
                  <a href="#">
                    <span class="profile-stats">Followers</span>
                    <span class="profile-value">100</span>
                  </a>
                </li>
              </ul>
            </div>
          </div><!-- End of Card -->
          
          <div id="settingsNav" class="card mt-4">
            <div class="card-header">Settings</div>
            <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
              <a class="nav-link active d-flex align-items-center" id="v-pills-home-tab" data-toggle="pill" href="#v-pills-home" role="tab" aria-controls="v-pills-home" aria-selected="true">Account <i class="fas fa-angle-right ml-auto"></i></a>
              <a class="nav-link d-flex align-items-center" id="v-pills-profile-tab" data-toggle="pill" href="#v-pills-profile" role="tab" aria-controls="v-pills-profile" aria-selected="false">Security <i class="fas fa-angle-right ml-auto"></i></a>
              <a class="nav-link d-flex align-items-center" id="v-pills-messages-tab" data-toggle="pill" href="#v-pills-messages" role="tab" aria-controls="v-pills-messages" aria-selected="false">Notifications <i class="fas fa-angle-right ml-auto"></i></a>
              <a class="nav-link d-flex align-items-center" id="v-pills-settings-tab" data-toggle="pill" href="#v-pills-settings" role="tab" aria-controls="v-pills-settings" aria-selected="false">Design <i class="fas fa-angle-right ml-auto"></i></a>
            </div>
          </div><!-- End of Card -->
      
        </div><!-- End of col-lg-3 col-md-4 -->
        
        <div class="col-lg-9 col-md-8">
            <!-- Nested row -->
            <div class="row">
              
              <div id="settings" class="col-xl-8 mt-4 mt-md-0">
                
                <!-- Settings -->
                <div class="tab-content" id="v-pills-tabContent">
                  
                  <div class="tab-pane fade show active" id="v-pills-home" role="tabpanel" aria-labelledby="v-pills-home-tab">
                    <nav class="nav-justified">
                      <div class="nav nav-tabs" id="nav-tab" role="tablist">
                        <a class="nav-item nav-link active" id="nav-user-tab" data-toggle="tab" href="#account-user" role="tab">User</a>
                        <a class="nav-item nav-link" id="nav-language-tab" data-toggle="tab" href="#account-language" role="tab">Language</a>
                        <a class="nav-item nav-link" id="nav-mobil-tab" data-toggle="tab" href="#account-mobile" role="tab">Mobile</a>
                      </div>
                    </nav>
                    
                    <div class="tab-content card" id="myTabContent">
                      <div class="tab-pane fade show active" id="account-user" role="tabpanel">
                        <div class="card-body">
                          Account User Pane
                        </div>
                      </div>
                      <div class="tab-pane fade" id="account-language" role="tabpanel">
                        <div class="card-body">
                          Account Lanuage Pane
                        </div>
                      </div>
                      <div class="tab-pane fade" id="account-mobile" role="tabpanel">
                        <div class="card-body">
                          Account Mobile Pane
                        </div>
                      </div>
                    </div>
                  </div>
                  
                  <div class="tab-pane fade" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab">
                  
                  </div>
                  
                  <div class="tab-pane fade" id="v-pills-messages" role="tabpanel" aria-labelledby="v-pills-messages-tab">
                  
                  </div>
                  
                  <div class="tab-pane fade" id="v-pills-settings" role="tabpanel" aria-labelledby="v-pills-settings-tab">
                  
                  </div>
                  
                </div>
                  

